generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  orders    Order[]
}

model Order {
  id          Int    @id @default(autoincrement())
  details     String
  status      String
  createdAt   DateTime @default(now())
  userId      Int
  user        User    @relation(fields: [userId], references: [id])
}
